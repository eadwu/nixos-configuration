{
  "name": "Ubuntu",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "forwardPorts": [],
  "runArgs": [
    "--userns=keep-id"
  ],
  "containerEnv": {
    "HOME": "/home/vscode"
  },
  "features": {
    "ghcr.io/devcontainers/features/nix:1": {
      "multiUser": true,
      "version": "latest"
    }
